{
  "cli": {
    "version": ">=15.0.0",
    "requireCommit": true,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      // https: //docs.expo.dev/build-reference/infrastructure/#android-server-images
      "bun": "1.2.4",
      /**
       * Environment variables defined in your eas.json build profile will be used when evaluating your app.config.js
       * when running eas build and will be available to all steps of the build process on the EAS Build server
       */
      "env": {
        "APP_VARIANT": "development",
        "EXPO_PUBLIC_API_BASE_URL": "https://dummyjson.com"
      },
      "environment": "development",
      "channel": "development",
      "distribution": "internal",
      "developmentClient": true
    },
    // only for "build:ios:dev:sim" script
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "bun": "1.2.4",
      "env": {
        "APP_VARIANT": "preview",
        "EXPO_PUBLIC_API_BASE_URL": "https://dummyjson.com"
      },
      "environment": "preview",
      "channel": "preview",
      "distribution": "internal"
    },
    "production": {
      "bun": "1.2.4",
      "env": {
        "APP_VARIANT": "production",
        "EXPO_PUBLIC_API_BASE_URL": "https://dummyjson.com"
      },
      "environment": "production",
      "channel": "production",
      "distribution": "store",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      // "android": {
      //   "serviceAccountKeyPath": "./service-account.json" // git ignored generatedservice account from google cloud
      //   "track": "internal"
      // }
      // "ios": {
      //   "appleId": "john@turtle.com",
      //   "ascAppId": "1234567890",
      //   "appleTeamId": "AB12XYZ34S"
      // }
    }
  }
}
