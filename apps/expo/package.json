  "main": "expo-router/entry",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "install:fix": "expo install --fix",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "dev": "expo start --dev-client",
    "dev:clean": "expo start --dev-client -c",
    "build:android": "eas build --profile development --platform android",
    "build:android:local": "eas build --profile development --platform android --local",
    "build:ios": "eas build --profile development-simulator --platform ios",
    "build:ios:local": "eas build --profile development-simulator --platform ios --local",
    "android": "expo run:android",
    "android:debug": "expo run:android --variant debug",
    "android:release": "expo run:android --variant release",
    "ios": "expo run:ios"
  },
