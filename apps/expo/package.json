{
  "name": "@workspace/expo",
  "version": "1.0.0",
  "private": false,
  "description": "Bulletproof Expo SDK 52 Template",
  "author": "Tri Rizeki Rifandani",
  "license": "MIT",
  "main": "index.ts",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "install:check": "expo install --check",
    "install:fix": "expo install --fix",
    "doctor": "bunx expo-doctor@latest",
    "prebuild": "expo prebuild --clean",
    "dev": "expo start --dev-client -c",
    "build:android": "eas build --profile development --platform android",
    "build:android:local": "eas build --profile development --platform android --local",
    "build:ios": "eas build --profile development-simulator --platform ios",
    "build:ios:local": "eas build --profile development-simulator --platform ios --local",
    "android": "expo run:android",
    "android:debug": "expo run:android --variant debug",
    "android:release": "expo run:android --variant release",
    "android:reverse:3000": "adb reverse tcp:3000 tcp:3000",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios -d"
  },
  "dependencies": {
    "@dev-plugins/react-navigation": "0.3.1",
