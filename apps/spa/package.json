{
  "name": "@workspace/spa",
  "type": "module",
  "version": "1.1.0",
  "private": true,
  "description": "Bulletproof React.js 19 Template",
  "author": "Tri Rizeki Rifandani",
  "license": "MIT",
  "scripts": {
    "sitemap:gen": "bun scripts/gen-sitemap.ts",
    "pwa-assets:gen": "pwa-assets-generator",
    "typecheck": "tsc --noEmit",
    "test:install": "playwright install --with-deps chromium",
    "test:report": "dotenvx run --env-file=.env.dev -- playwright show-report",
    "test": "dotenvx run --env DEBUG=pw:api --env-file=.env.dev -- playwright test",
    "test:ui": "dotenvx run --env-file=.env.dev -- playwright test --ui --headed",
    "test:ci": "dotenvx run --env-file=.env.dev -- playwright test --forbid-only --shard=$SHARD_INDEX/$SHARD_TOTAL",
    "meta": "bunx check-site-meta 3001 -p 3051",
    "dev": "cp .env.dev .env.local && vite dev",
    "dev:prod": "cp .env.prod .env.local && vite dev --mode production",
    "build": "cp .env.dev .env.local && vite build --mode development",
    "build:prod": "cp .env.prod .env.local && vite build",
    "preview": "vite preview",
    "build-and-preview": "run-s build preview"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.51.1",
    "@formkit/auto-animate": "0.9.0",
    "@hookform/resolvers": "5.2.2",
    "@internationalized/date": "3.10.0",
    "@openfeature/flagsmith-client-provider": "0.1.3",
    "@openfeature/react-sdk": "1.0.2",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.208.0",
    "@opentelemetry/instrumentation": "0.208.0",
    "@opentelemetry/instrumentation-fetch": "0.208.0",
    "@opentelemetry/opentelemetry-browser-detector": "0.208.0",
    "@opentelemetry/resources": "2.2.0",
    "@opentelemetry/sdk-metrics": "2.2.0",
    "@opentelemetry/sdk-trace-web": "2.2.0",
    "@opentelemetry/semantic-conventions": "1.38.0",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@t3-oss/env-core": "0.13.8",
    "@tanstack/react-query": "5.90.7",
    "@tanstack/react-query-devtools": "5.90.2",
    "@tanstack/react-router": "1.134.18",
    "@tanstack/react-router-devtools": "1.134.18",
    "@unhead/react": "2.0.19",
    "@unhead/schema-org": "2.0.19",
    "@workspace/core": "workspace:*",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "ky": "1.14.0",
    "motion": "12.23.24",
    "radashi": "12.7.0",
    "react": "19.2.0",
    "react-aria": "3.44.0",
    "react-aria-components": "1.13.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.66.0",
    "react-stately": "3.42.0",
    "recharts": "3.3.0",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "tailwind-variants": "3.1.1",
    "ts-pattern": "5.9.0",
    "tw-animate-css": "1.4.0",
    "type-fest": "5.2.0",
