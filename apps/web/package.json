{
  "name": "@workspace/web",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "Bulletproof nextjs 15 template",
  "author": "Tri Rizeki Rifandani",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "DEBUG=pw:api playwright test",
    "test:ui": "playwright test --ui --headed",
    "test:report": "playwright show-report",
    "test:install": "playwright install --with-deps chromium",
    "dev": "cp .env.development .env.local && next dev --port 3002",
    "dev:turbo": "cp .env.development .env.local && next dev --turbopack --port 3002",
    "build": "cp .env.production .env.local && next build",
    "start": "next start"
  },
  "dependencies": {
    "@iconify/react": "5.1.0",
    "@internationalized/date": "3.6.0",
    "@workspace/core": "workspace:*",
    "ky": "1.7.4",
    "next": "15.1.2",
    "next-themes": "0.4.4",
    "radashi": "12.3.0",
    "react": "19.0.0",
    "react-aria": "3.36.0",
    "react-aria-components": "1.5.0",
    "react-dom": "19.0.0",
    "react-stately": "3.34.0",
    "server-only": "0.0.1",
    "sonner": "1.7.1",
    "tailwind-merge": "2.6.0",
    "tailwind-variants": "0.3.1",
    "ts-pattern": "5.6.0",
    "zod": "3.24.1",
    "zod-validation-error": "3.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "9.3.0",
    "@playwright/test": "1.49.1",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/postcss": "4.0.0",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.0.0",
    "@types/node": "22.10.2",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@workspace/typescript-config": "workspace:*",
    "postcss": "8.5.1",
    "tailwindcss": "4.0.0",
    "tailwindcss-animate": "1.0.7",
    "tailwindcss-react-aria-components": "1.2.0",
    "type-fest": "4.30.2",
    "typescript": "5.7.2"
  }
}
