{
  "name": "@workspace/web",
  "type": "module",
  "version": "1.1.0",
  "private": true,
  "description": "Bulletproof Next.js 15 Template",
  "author": "Tri Rizeki Rifandani",
  "license": "MIT",
  "scripts": {
    "pwa:assets-gen": "pwa-assets-generator --preset minimal-2023 public/favicon.svg",
    "typecheck": "tsc --noEmit",
    "db:pull": "drizzle-kit pull",
    "db:push": "drizzle-kit push --verbose --strict",
    "db:gen": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio --port 3003 --verbose",
    "auth:gen": "dotenvx run --env-file=.env.dev -- bunx @better-auth/cli generate --config ./src/auth/utils/auth.ts",
    "test:install": "playwright install --with-deps chromium",
    "test:report": "dotenvx run --env-file=.env.dev -- playwright show-report",
    "test": "dotenvx run --env-file=.env.dev -- playwright test",
    "test:ui": "dotenvx run --env-file=.env.dev -- playwright test --ui --headed",
    "test:ci": "dotenvx run --env-file=.env.dev -- playwright test --forbid-only --shard=$SHARD_INDEX/$SHARD_TOTAL",
    "meta": "bunx check-site-meta 3002 -p 3052",
    "dev": "cp .env.dev .env.local && next dev --port 3002",
    "dev:prod": "cp .env.prod .env.local && next dev --port 3002",
    "dev:turbo": "cp .env.dev .env.local && next dev --turbopack --port 3002",
    "build": "cp .env.dev .env.local && next build",
    "build:prod": "cp .env.prod .env.local && next build",
    "build:prod:analyze": "cp .env.prod .env.local && dotenvx run --env ANALYZE=true --env-file=.env.prod -- next build",
    "start": "next start",
    "build-and-preview": "bun run build && bun run start"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.45.1",
    "@formkit/auto-animate": "0.8.2",
    "@hookform/resolvers": "5.1.1",
    "@internationalized/date": "3.8.2",
    "@next-safe-action/adapter-react-hook-form": "2.0.0",
    "@next/bundle-analyzer": "15.3.4",
    "@nosecone/next": "1.0.0-beta.8",
    "@openfeature/flagsmith-client-provider": "0.1.3",
    "@openfeature/react-sdk": "1.0.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/api-logs": "0.203.0",
    "@opentelemetry/exporter-logs-otlp-http": "0.203.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.203.0",
    "@opentelemetry/instrumentation-dns": "0.47.0",
    "@opentelemetry/instrumentation-http": "0.203.0",
    "@opentelemetry/instrumentation-net": "0.47.0",
    "@opentelemetry/instrumentation-pg": "0.55.0",
    "@opentelemetry/instrumentation-runtime-node": "0.17.0",
    "@opentelemetry/instrumentation-undici": "0.14.0",
    "@opentelemetry/resources": "2.0.1",
    "@opentelemetry/sdk-logs": "0.203.0",
    "@opentelemetry/sdk-metrics": "2.0.1",
    "@opentelemetry/semantic-conventions": "1.36.0",
    "@radix-ui/react-scroll-area": "1.2.9",
    "@t3-oss/env-nextjs": "0.13.8",
    "@tanstack/react-query": "5.81.5",
    "@tanstack/react-query-devtools": "5.81.5",
    "@vercel/otel": "1.13.0",
    "@workspace/core": "workspace:*",
    "better-auth": "1.3.2",
    "drizzle-orm": "0.44.3",
    "drizzle-seed": "0.3.1",
    "drizzle-zod": "0.8.2",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "ky": "1.8.1",
    "motion": "12.19.2",
    "next": "15.3.4",
    "next-intl": "4.3.1",
    "next-safe-action": "8.0.7",
    "next-themes": "0.4.6",
    "nuqs": "2.4.3",
    "pg": "8.16.3",
    "radashi": "12.6.0",
    "react": "19.1.0",
    "react-aria": "3.41.1",
    "react-aria-components": "1.10.1",
    "react-dom": "19.1.0",
    "react-hook-form": "7.59.0",
    "react-stately": "3.39.0",
    "recharts": "3.0.2",
    "schema-dts": "1.1.5",
    "server-only": "0.0.1",
    "sonner": "2.0.5",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "1.0.0",
    "ts-pattern": "5.7.1",
    "tw-animate-css": "1.3.4",
    "type-fest": "4.41.0",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@faker-js/faker": "9.8.0",
    "@hookform/devtools": "4.4.0",
    "@iconify/react": "6.0.0",
    "@playwright/test": "1.53.1",
    "@tailwindcss/postcss": "4.1.11",
    "@types/node": "24.0.7",
    "@types/pg": "8.15.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vite-pwa/assets-generator": "1.0.0",
    "@workspace/typescript-config": "workspace:*",
    "drizzle-kit": "0.31.4",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.11",
    "tailwindcss-react-aria-components": "2.0.0",
    "typescript": "5.8.3"
  }
}
