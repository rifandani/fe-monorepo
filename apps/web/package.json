{
  "name": "@workspace/web",
  "type": "module",
  "version": "1.1.0",
  "private": true,
  "description": "Bulletproof Next.js 15 Template",
  "author": "Tri Rizeki Rifandani",
  "license": "MIT",
  "scripts": {
    "pwa-assets:gen": "pwa-assets-generator",
    "typecheck": "next typegen && tsc --noEmit",
    "db:pull": "dotenvx run --env-file=.env.dev -- drizzle-kit pull",
    "db:push": "dotenvx run --env-file=.env.dev -- drizzle-kit push --verbose --strict",
    "db:gen": "dotenvx run --env-file=.env.dev -- drizzle-kit generate",
    "db:migrate": "dotenvx run --env-file=.env.dev -- drizzle-kit migrate",
    "db:studio": "dotenvx run --env-file=.env.dev -- drizzle-kit studio --port 3003 --verbose",
    "auth:gen": "dotenvx run --env-file=.env.dev -- bunx @better-auth/cli generate --config ./src/auth/utils/auth.ts --output ./src/db/auth-schema.ts --yes",
    "test:install": "playwright install --with-deps chromium",
    "test:report": "dotenvx run --env-file=.env.dev -- playwright show-report",
    "test": "dotenvx run --env-file=.env.dev -- playwright test",
    "test:ui": "dotenvx run --env-file=.env.dev -- playwright test --ui --headed",
    "test:ci": "dotenvx run --env-file=.env.dev -- playwright test --forbid-only --shard=$SHARD_INDEX/$SHARD_TOTAL",
    "meta": "bunx check-site-meta 3002 -p 3052",
    "dev": "cp .env.dev .env.local && next dev --turbopack --port 3002",
    "dev:prod": "cp .env.prod .env.local && next dev --turbopack --port 3002",
    "build": "cp .env.dev .env.local && next build --turbopack",
    "build:prod": "cp .env.prod .env.local && next build --turbopack",
    "build:prod:analyze": "cp .env.prod .env.local && dotenvx run --env ANALYZE=true --env-file=.env.prod -- next build --turbopack",
    "start": "next start",
    "build-and-preview": "bun run build && bun run start"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.49.0",
    "@formkit/auto-animate": "0.9.0",
    "@hookform/resolvers": "5.2.1",
    "@internationalized/date": "3.9.0",
    "@next-safe-action/adapter-react-hook-form": "2.0.0",
    "@next/bundle-analyzer": "15.5.3",
    "@nosecone/next": "1.0.0-beta.11",
    "@openfeature/flagsmith-client-provider": "0.1.3",
    "@openfeature/react-sdk": "1.0.1",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/api-logs": "0.203.0",
    "@opentelemetry/exporter-logs-otlp-http": "0.205.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.205.0",
    "@opentelemetry/instrumentation-dns": "0.49.0",
    "@opentelemetry/instrumentation-http": "0.205.0",
    "@opentelemetry/instrumentation-net": "0.49.0",
    "@opentelemetry/instrumentation-pg": "0.58.0",
    "@opentelemetry/instrumentation-runtime-node": "0.19.0",
    "@opentelemetry/instrumentation-undici": "0.16.0",
    "@opentelemetry/resources": "2.0.1",
    "@opentelemetry/sdk-logs": "0.203.0",
    "@opentelemetry/sdk-metrics": "2.0.1",
    "@opentelemetry/semantic-conventions": "1.37.0",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@t3-oss/env-nextjs": "0.13.8",
    "@tanstack/react-query": "5.87.4",
    "@tanstack/react-query-devtools": "5.87.4",
    "@vercel/otel": "1.13.0",
    "@workspace/core": "workspace:*",
    "better-auth": "1.3.9",
    "drizzle-orm": "0.44.5",
    "drizzle-seed": "0.3.1",
    "drizzle-zod": "0.8.3",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "ky": "1.10.0",
    "motion": "12.23.12",
    "next": "15.5.3",
    "next-intl": "4.3.8",
    "next-safe-action": "8.0.11",
    "next-themes": "0.4.6",
    "nuqs": "2.6.0",
    "pg": "8.16.3",
    "radashi": "12.6.2",
    "react": "19.1.1",
    "react-aria": "3.43.2",
    "react-aria-components": "1.12.2",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "react-stately": "3.41.0",
    "recharts": "3.2.0",
    "schema-dts": "1.1.5",
    "server-only": "0.0.1",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "3.1.1",
    "ts-pattern": "5.8.0",
    "tw-animate-css": "1.3.8",
    "type-fest": "4.41.0",
    "zod": "4.1.8"
  },
  "devDependencies": {
    "@faker-js/faker": "10.0.0",
    "@hookform/devtools": "4.4.0",
    "@iconify/react": "6.0.1",
    "@playwright/test": "1.55.0",
    "@tailwindcss/postcss": "4.1.13",
    "@types/node": "24.3.3",
    "@types/pg": "8.15.5",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vite-pwa/assets-generator": "1.0.1",
    "@workspace/typescript-config": "workspace:*",
    "drizzle-kit": "0.31.4",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.13",
    "tailwindcss-react-aria-components": "2.0.1",
    "typescript": "5.9.2"
  },
  "overrides": {
