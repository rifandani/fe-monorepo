{
  "name": "fe-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.2.2",
  "description": "Hono, React, Next.js, Expo monorepo template",
  "author": "Tri Rizeki Rifandani",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prepare": "husky",
    "clean": "rimraf --glob \"**/node_modules\" \"**/dev-dist\" \"**/dist\" \"**/build\" \"**/html\" \"**/playwright\" \"**/playwright-report\" \"**/playwright-test-results\" \"**/bun.lock\" \"**/.next\" && bun install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspector": "eslint --inspect-config",
    "typecheck": "run-p web:typecheck spa:typecheck expo:typecheck hono:typecheck",
    "regression": "run-p lint web:typecheck spa:typecheck expo:typecheck",
    "web:dev": "bun --filter @workspace/web dev",
    "web:dev:prod": "bun --filter @workspace/web dev:prod",
    "web:build": "bun --filter @workspace/web build",
    "web:build:prod": "bun --filter @workspace/web build:prod",
    "web:build:prod:analyze": "bun --filter @workspace/web build:prod:analyze",
    "web:typecheck": "bun --filter @workspace/web typecheck",
    "web:test:install": "bun --filter @workspace/web test:install",
    "web:test:report": "bun --filter @workspace/web test:report",
    "web:test": "bun --filter @workspace/web test",
    "web:test:ui": "bun --filter @workspace/web test:ui",
    "web:test:ci": "bun --filter @workspace/web test:ci",
    "spa:dev": "bun --filter @workspace/spa dev",
    "spa:build": "bun --filter @workspace/spa build",
    "spa:typecheck": "bun --filter @workspace/spa typecheck",
    "spa:test:install": "bun --filter @workspace/spa test:install",
    "spa:test": "bun --filter @workspace/spa test",
    "spa:test:ui": "bun --filter @workspace/spa test:ui",
    "spa:test:ci": "bun --filter @workspace/spa test:ci",
    "expo:typecheck": "bun --filter @workspace/expo typecheck",
    "expo:prebuild": "bun --filter @workspace/expo prebuild",
    "expo:prebuild:clean": "bun --filter @workspace/expo prebuild:clean",
    "expo:dev": "bun --filter @workspace/expo dev",
    "expo:dev:clean": "bun --filter @workspace/expo dev:clean",
    "expo:build:android": "bun --filter @workspace/expo build:android",
    "expo:build:android:local": "bun --filter @workspace/expo build:android:local",
    "expo:build:ios": "bun --filter @workspace/expo build:ios",
    "expo:build:ios:local": "bun --filter @workspace/expo build:ios:local",
    "expo:android": "bun --filter @workspace/expo android",
    "expo:ios": "bun --filter @workspace/expo ios",
    "hono:typecheck": "bun --filter @workspace/hono typecheck",
    "hono:dev": "bun --filter @workspace/hono dev",
    "hono:node:dev": "bun --filter @workspace/hono node:dev",
    "hono:node:build": "bun --filter @workspace/hono node:build",
    "hono:node:start": "bun --filter @workspace/hono node:start"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.3.0",
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@eslint-react/eslint-plugin": "1.29.0",
    "@eslint/compat": "1.2.7",
    "@eslint/eslintrc": "3.3.0",
    "@next/eslint-plugin-next": "15.2.0",
    "@prettier/plugin-xml": "3.4.1",
    "@tanstack/eslint-plugin-router": "1.105.0",
    "@workspace/typescript-config": "workspace:*",
    "bumpp": "10.0.3",
    "eslint": "9.21.0",
    "eslint-plugin-depend": "0.12.0",
    "eslint-plugin-format": "1.0.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-tailwindcss": "3.18.0",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "npm-run-all2": "7.0.2",
    "rimraf": "6.0.1",
    "typescript": "5.8.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "trustedDependencies": [
    "@swc/core",
    "sharp",
    "esbuild"
  ]
}
